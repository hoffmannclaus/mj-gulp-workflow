		"autoprefixer": "^8.6.5",
		"babel-core": "^6.26.3",
		"babel-preset-env": "^1.7.0",
		"cli-real-favicon": "0.0.6",
		"del": "^3.0.0",
		"glob": "^7.1.2",
		"gulp": "^3.9.1",
		"gulp-babel": "^7.0.1",
		"gulp-browserify": "^0.5.1",
		"gulp-changed": "^3.2.0",
		"gulp-clean-css": "^3.10.0",
		"gulp-concat": "^2.6.1",
		"gulp-imagemin": "^4.1.0",
		"gulp-notify": "^3.2.0",
		"gulp-postcss": "^7.0.1",
		"gulp-sass": "^3.2.1",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-svgmin": "^1.2.4",
		"gulp-util": "^3.0.8",
		"gulp-watch": "^5.0.1",
		"http-server": "^0.11.1",
		"imagemin-pngquant": "^5.1.0",
		"inquirer": "^6.1.0",
		"junk": "^2.1.0",
