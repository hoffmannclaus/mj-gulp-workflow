{
	"name": "mj-gulp-workflow",
	"version": "2.0.0",
	"repository": "https://github.com/moritzjacobs/mj-gulp-workflow",
	"description": "A fork of https://github.com/zephir/zephir-gulp-workflow with added sourcemaps and other things.",
	"main": "dist/index.js",
	"scripts": {
		"esw": "esw --fix -w",
		"cli": "npm run start",
		"dev": "cd ./test; gulp",
		"dist": "cd ./test; gulp dist --env dist",
		"pretest": "npm run babel-dist; npm run clean; cd ./test; gulp test --env dist",
		"test": "jest",
		"jest": "jest",
		"clean": "rm -rf ./test/output",
		"testd": "http-server ./test -o",
		"babel-dist": "babel src -d dist",
		"start": "babel-node src --presets=env"
	},
	"jest": {
		"verbose": true,
		"testURL": "http://localhost/"
	},
	"contributors": [
		"Marc Stampfli <marc.stampfli@zephir.ch> (http://zephir.ch)",
		"Moritz Jacobs <mail@moritzjacobs.de> (https://moritzjacobs.de)"
	],
	"license": "ISC",
	"dependencies": {
		"@babel/core": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"app-root-dir": "^1.0.2",
		"autoprefixer": "^9.1.3",
		"cli-real-favicon": "0.0.6",
		"del": "^3.0.0",
		"fancy-log": "^1.3.2",
		"glob": "^7.1.3",
		"gulp": "^4.0.0",
		"gulp-babel": "^8.0.0",
		"gulp-bro": "^1.0.3",
		"gulp-clean-css": "^3.10.0",
		"gulp-concat": "^2.6.1",
		"gulp-imagemin": "^4.1.0",
		"gulp-notify": "^3.2.0",
		"gulp-postcss": "^8.0.0",
		"gulp-sass": "^4.0.1",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-svgmin": "^2.0.0",
		"gulp-watch": "^5.0.1",
		"http-server": "^0.11.1",
		"imagemin-pngquant": "^6.0.0",
		"junk": "^2.1.0",
		"merge-stream": "^1.0.1",
		"rfg-api": "^0.3.0",
		"uniqid": "^5.0.3",
		"yargs": "^12.0.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.0.0",
		"@babel/core": "^7.0.0",
		"@babel/node": "^7.0.0",
		"babel-core": "^7.0.0-bridge.0",
		"eslint": "^5.5.0",
		"eslint-config-standard": "^12.0.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jest": "^21.22.0",
		"eslint-plugin-node": "^7.0.1",
		"eslint-plugin-promise": "^4.0.0",
		"eslint-plugin-standard": "^4.0.0",
		"eslint-watch": "^4.0.2",
		"jest": "^23.5.0"
	}
}
